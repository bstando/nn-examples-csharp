<Window x:Class="Rozpoznawanie_Cyfr_Adaline.Wykres"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:Rozpoznawanie_Cyfr_Adaline"
        Title="Wykres" Height="600" Width="1024">
    <Window.DataContext>
        <local:ModelWidoku></local:ModelWidoku>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Title="{Binding Title}">
            <oxy:PlotView.Series>
                <oxy:LineSeries ItemsSource="{Binding Points}"/>
            </oxy:PlotView.Series>
        </oxy:PlotView>
    </Grid>
</Window>
